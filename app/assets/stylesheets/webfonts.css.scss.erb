@font-face {
    font-family: 'FederoRegular';
    src: url(<%= asset_path 'federo-regular-webfont.eot' %>);
    src: url(<%= asset_path 'federo-regular-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'federo-regular-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'federo-regular-webfont.ttf' %>) format('truetype');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'KaushanScriptRegular';
    src: url(<%= asset_path 'KaushanScript-Regular-webfont.eot' %>);
    src: url(<%= asset_path 'KaushanScript-Regular-webfont.eot?#iefix' %>) format('embedded-opentype'),
         url(<%= asset_path 'KaushanScript-Regular-webfont.woff' %>) format('woff'),
         url(<%= asset_path 'KaushanScript-Regular-webfont.ttf' %>) format('truetype'),
         url(<%= asset_path 'KaushanScript-Regular-webfont.svg#KaushanScriptRegular' %>) format('svg');
    font-weight: normal;
    font-style: normal;
}